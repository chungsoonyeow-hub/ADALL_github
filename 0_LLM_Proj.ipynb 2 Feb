      "cell_type": "code",
      "source": [
        "# Decision Tree Baseline\n",
        "import pandas as pd\n",
        "import numpy as np\n",
        "\n",
        "####Preprocessing pipelines\n",
        "\n",
        "from sklearn.compose import ColumnTransformer\n",
        "from sklearn.pipeline import Pipeline\n",
        "from sklearn.tree import DecisionTreeClassifier\n",
        "from sklearn.impute import SimpleImputer\n",
        "from sklearn.preprocessing import OrdinalEncoder\n",
        "\n",
        "from sklearn.model_selection import cross_validate, StratifiedKFold\n",
        "from sklearn.metrics import confusion_matrix, classification_report\n",
        "\n",
        "numeric_transformer = Pipeline(steps=[\n",
        "    ('imputer', SimpleImputer(strategy='median'))\n",
        "])\n",
        "\n",
        "categorical_transformer = Pipeline(steps=[\n",
        "    ('imputer', SimpleImputer(strategy='most_frequent')),\n",
        "    ('encoder', OrdinalEncoder(\n",
        "        handle_unknown='use_encoded_value',\n",
        "        unknown_value=-1\n",
        "    ))\n",
        "])\n",
        "\n",
        "preprocessor = ColumnTransformer(\n",
        "    transformers=[\n",
        "        ('num', numeric_transformer, num_cols),\n",
        "        ('cat', categorical_transformer, cat_cols)\n",
        "    ]\n",
        ")\n",
        "#Baseline Decision Tree (classification)\n",
        "dt_model = DecisionTreeClassifier(\n",
        "    max_depth=5,\n",
        "    min_samples_leaf=50,\n",
        "    class_weight='balanced',\n",
        "    random_state=42\n",
        ")\n",
        "\n",
        "##Full pipeline (THIS is what CV and fit will use)\n",
        "dt_pipeline = Pipeline(steps=[\n",
        "    ('preprocessor', preprocessor),\n",
        "    ('model', dt_model)\n",
        "])\n",
        "\n",
        "\n",
        "####Cross-validation on TRAIN set\n",
        "#####This is the “CV step” from the notebook\n",
        "\n",
        "print(\"=== DECISION TREE: CROSS-VALIDATION (TRAIN SET) ===\")\n",
        "cv = StratifiedKFold(n_splits=10, shuffle=True, random_state=42)\n",
        "#cv = StratifiedKFold(n_splits=5, shuffle=True, random_state=42)\n",
        "\n",
        "cv_results = cross_validate(\n",
        "    dt_pipeline,\n",
        "    X_train,\n",
        "    y_train,\n",
        "    cv=cv,\n",
        "    scoring=['accuracy', 'precision', 'recall', 'f1'],\n",
        "    return_train_score=False\n",
        ")\n",
        "\n",
        "dt_cv_summary = {\n",
        "    'model': 'Decision Tree (baseline)',\n",
        "    'cv_accuracy': cv_results['test_accuracy'].mean(),\n",
        "    'cv_precision': cv_results['test_precision'].mean(),\n",
        "    'cv_recall': cv_results['test_recall'].mean(),\n",
        "    'cv_f1': cv_results['test_f1'].mean()\n",
        "}\n",
        "\n",
        "for k, v in dt_cv_summary.items():\n",
        "    if k != 'model':\n",
        "        print(k, round(v, 3))\n",
        "\n",
        "\n",
        "## fit to model\n",
        "print(\"\\n=== FINAL FIT: DECISION TREE ON FULL TRAIN SET ===\")\n",
        "dt_pipeline.fit(X_train, y_train)\n",
        "\n",
        "######################  New ######################3\n",
        "cv_results = cross_validate(\n",
        "    dt_pipeline, X_train, y_train, cv=cv,\n",
        "    scoring=['f1'],\n",
        "    return_train_score=True  # Important!\n",
        ")\n",
        "\n",
        "#################################################################\n",
        "\n",
        "y_pred = dt_pipeline.predict(X_test)\n",
        "\n",
        "## storing results\n",
        "report = classification_report(y_test, y_pred, output_dict=True)\n",
        "\n",
        "############### New #################################\n",
        "# 1. Identify the index of the best performing set of parameters\n",
        "idx = dt_grid.best_index_\n",
        "\n",
        "# 2. Extract internal CV metrics (The Diagnostic Layer)\n",
        "cv_f1_train_mean = dt_grid.cv_results_['mean_train_score'][idx]\n",
        "cv_f1_train_std = dt_grid.cv_results_['std_train_score'][idx]\n",
        "cv_f1_val_mean = dt_grid.best_score_\n",
        "cv_f1_val_std = dt_grid.cv_results_['std_test_score'][idx]\n",
        "\n",
        "# 3. Predict on Holdout/Test set\n",
        "y_pred_best = dt_grid.best_estimator_.predict(X_test)\n",
        "report_best = classification_report(y_test, y_pred_best, output_dict=True)\n",
        "\n",
        "# 4. Store everything in the results list\n",
        "# We use long, descriptive keys so you know exactly what to drop later\n",
        "results.append({\n",
        "    'Cycle': 'Tuned_DT_Tournament',\n",
        "    'Model_Type': 'Decision Tree',\n",
        "\n",
        "    # Academic/Diagnostic Metrics (Lab Style)\n",
        "    'cv_f1_train_avg': round(cv_f1_train_mean, 4),\n",
        "    'cv_f1_train_std': round(cv_f1_train_std, 4),\n",
        "    'cv_f1_val_avg': round(cv_f1_val_mean, 4),\n",
        "    'cv_f1_val_std': round(cv_f1_val_std, 4),\n",
        "\n",
        "    # Practical/Business Metrics (Project Style)\n",
        "    'holdout_accuracy': round(report_best['accuracy'], 4),\n",
        "    'holdout_f1_breach': round(report_best['False']['f1-score'], 4),\n",
        "    'holdout_recall_breach': round(report_best['False']['recall'], 4),\n",
        "    'holdout_precision_breach': round(report_best['False']['precision'], 4),\n",
        "\n",
        "    # Hyperparameters\n",
        "    'best_params': str(dt_grid.best_params_)\n",
        "})\n",
        "\n",
        "# 5. Display horizontally to see all columns\n",
        "results_df = pd.DataFrame(results)\n",
        "display(results_df)\n",
        "\n",
        "\n",
        "####################################################\n",
        "\n",
        "# dt_results = {\n",
        "# \"cv_f1_val\": cv_results['test_f1'].mean(),\n",
