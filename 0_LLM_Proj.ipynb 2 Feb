        "# RECREATE from raw (140k → 24k agg)\n",
        "\n",
        "\n",
        "#Aggregation (latest per incident)\n",
        "df_agg = df.sort_values(['number', 'sys_updated_at']).groupby('number').last().reset_index()\n",
        "print(f\"AGGREGATED: {df_agg.shape}\")  # Expect ~24k x 36\n",
