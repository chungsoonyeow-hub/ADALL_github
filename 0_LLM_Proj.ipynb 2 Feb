      "cell_type": "code",
      "source": [
        "### Fine Tune Random Forest\n",
        "\n",
        "# --- RANDOM FOREST: FURTHER TUNING (MIXED ENCODING) ---\n",
        "\n",
        "# 1. Model Definition\n",
        "rf_model_fine = RandomForestClassifier(\n",
        "    n_estimators=100,\n",
        "    class_weight=\"balanced\",\n",
        "    random_state=42,\n",
        "    n_jobs=-1\n",
        ")\n",
        "\n",
        "# 2. Pipeline Amendment (Pointing to the new preprocessor name)\n",
        "rf_pipeline_fine = Pipeline(steps=[\n",
        "    (\"preprocessor\", preprocessor_mixed), # AMENDMENT 1: Using the Refined Preprocessor\n",
        "    (\"model\", rf_model_fine)\n",
        "])\n",
        "\n",
        "# 3. Param Grid (Including Subsample and Colsample logic)\n",
        "param_grid_rf = {\n",
        "    \"model__max_depth\": [10, 20, None],\n",
        "    \"model__min_samples_leaf\": [20, 50],\n",
        "    \"model__max_features\": [0.6, 0.8, \"sqrt\"], # Stochastic 'Colsample'\n",
        "    \"model__bootstrap\": [True],\n",
        "    \"model__max_samples\": [0.7, 0.8, None]     # Stochastic 'Subsample'\n",
        "}\n",
        "\n",
        "# 4. Grid Search Execution\n",
        "print(\"=== RANDOM FOREST: GRID SEARCH (REFINED ENCODING) ===\")\n",
        "cv_strat = StratifiedKFold(n_splits=5, shuffle=True, random_state=42)\n",
        "\n",
        "rf_grid_fine = GridSearchCV(\n",
        "    estimator=rf_pipeline_fine,\n",
        "    param_grid=param_grid_rf,\n",
        "    scoring=\"f1\",\n",
        "    cv=cv_strat,\n",
        "    n_jobs=-1,\n",
        "    refit=True\n",
        ")\n",
        "\n",
        "rf_grid_fine.fit(X_train, y_train)\n",
        "\n",
        "# 5. Extract and Evaluate\n",
        "best_rf_fine = rf_grid_fine.best_estimator_\n",
        "y_pred_rf_fine = best_rf_fine.predict(X_test)\n",
        "report_rf_fine = classification_report(y_test, y_pred_rf_fine, output_dict=True)\n",
        "\n",
        "# 6. Append to Table (AMENDMENT 2: Clearly labeling the new row)\n",
        "results.append({\n",
        "    \"model\": \"Random Forest (Further Tuning - Mixed)\",\n",
        "    \"cv_f1\": rf_grid_fine.best_score_,\n",
        "    \"test_accuracy\": report_rf_fine[\"accuracy\"],\n",
        "    \"test_recall_breach\": report_rf_fine[\"False\"][\"recall\"],\n",
        "    \"test_f1_breach\": report_rf_fine[\"False\"][\"f1-score\"],\n",
        "    \"best_params\": str(rf_grid_fine.best_params_)\n",
        "})\n",
        "\n",
        "display(pd.DataFrame(results))"
