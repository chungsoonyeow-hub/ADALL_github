    {
      "cell_type": "code",
      "source": [
        "##Fine tuning Decision Tree\n",
        "\n",
        "import pandas as pd\n",
        "import numpy as np\n",
        "from sklearn.compose import ColumnTransformer\n",
        "from sklearn.pipeline import Pipeline\n",
        "from sklearn.tree import DecisionTreeClassifier\n",
        "from sklearn.impute import SimpleImputer\n",
        "from sklearn.preprocessing import OrdinalEncoder, OneHotEncoder # Added OneHot\n",
        "from sklearn.model_selection import StratifiedKFold, GridSearchCV, cross_validate\n",
        "from sklearn.metrics import confusion_matrix, classification_report\n",
        "\n",
        "# 1. DEFINE YOUR COLUMN CATEGORIES\n",
        "\n",
        "\n",
        "# 1. ORDINAL (Ranked items)\n",
        "ord_cols = ['impact', 'urgency', 'priority']\n",
        "\n",
        "# 2. ONE-HOT (Categorical names)\n",
        "ohe_cols = [\n",
        "    'contact_type',\n",
        "    'category',\n",
        "    'location_grp',\n",
        "    'subcategory_grp',\n",
        "    'assignment_group_grp'\n",
        "]\n",
        "\n",
        "# 3. NUMERICAL (The ones with numbers in the screenshot)\n",
        "num_cols = [\n",
        "    'opened_hour',\n",
        "    'opened_dayofweek',\n",
        "    'is_weekend',\n",
        "    'is_business_hours',\n",
        "    'has_cmdb_ci',\n",
        "    'has_symptom_text'\n",
        "]\n",
        "\n",
        "\n",
        "\n",
        "# 2. PREPROCESSING PIPELINES\n",
        "numeric_transformer = Pipeline(steps=[\n",
        "    ('imputer', SimpleImputer(strategy='median'))\n",
        "])\n",
        "\n",
        "ordinal_transformer = Pipeline(steps=[\n",
        "    ('imputer', SimpleImputer(strategy='most_frequent')),\n",
        "    ('encoder', OrdinalEncoder(handle_unknown='use_encoded_value', unknown_value=-1))\n",
        "])\n",
        "\n",
        "nominal_transformer = Pipeline(steps=[\n",
        "    ('imputer', SimpleImputer(strategy='most_frequent')),\n",
        "    ('encoder', OneHotEncoder(handle_unknown='ignore')) # 'ignore' handles new categories in test data\n",
        "])\n",
        "\n",
        "# 3. THE MIXED PREPROCESSOR (This is your main amendment!)\n",
        "\n",
        "preprocessor_mixed= ColumnTransformer(\n",
        "    transformers=[\n",
        "        ('num', numeric_transformer, num_cols),\n",
        "        ('ord', ordinal_transformer, ord_cols),\n",
        "        ('ohe', nominal_transformer, ohe_cols)\n",
        "    ]\n",
        ")\n",
        "\n",
        "# 4. THE MODEL & PIPELINE\n",
        "dt_model = DecisionTreeClassifier(\n",
        "    class_weight='balanced', # Crucial for your 93/7 split\n",
        "    random_state=42\n",
        ")\n",
        "\n",
        "dt_pipeline = Pipeline(steps=[\n",
        "    ('preprocessor', preprocessor_mixed),\n",
        "    ('model', dt_model)\n",
        "])\n",
        "\n",
        "# 5. THE NEW PARAMS FOR GRID SEARCH\n",
        "# We expanded these to give the model more 'room' to learn\n",
        "# param_grid = {\n",
        "#     'model__max_depth': [3, 5, 8, 12],\n",
        "#     'model__min_samples_leaf': [20, 50, 100],\n",
        "#     'model__criterion': ['gini', 'entropy']\n",
        "# }\n",
        "\n",
        "param_grid = {\n",
        "    # How deep the tree goes (Complexity)\n",
        "    'model__max_depth': [3, 5, 8, 12, 15],\n",
        "    # Minimum samples needed to split a node (Prevents tiny, noisy branches)\n",
        "    'model__min_samples_split': [10, 20, 50],\n",
        "    # Minimum samples allowed in a 'leaf' (The end of a branch)\n",
        "    'model__min_samples_leaf': [10, 20, 50, 100],\n",
        "    # The math used to measure the quality of the split\n",
        "    'model__criterion': ['gini', 'entropy']\n",
        "}\n",
        "\n",
        "# 6. RUN GRID SEARCH\n",
        "print(\"=== DECISION TREE: GRID SEARCH CV (MIXED ENCODING) ===\")\n",
        "cv = StratifiedKFold(n_splits=10, shuffle=True, random_state=42)\n",
        "\n",
        "dt_grid = GridSearchCV(\n",
        "    estimator=dt_pipeline,\n",
        "    param_grid=param_grid,\n",
        "    cv=cv,\n",
        "    scoring='f1',\n",
        "    n_jobs=-1,\n",
        "    refit=True\n",
        ")\n",
        "\n",
        "dt_grid.fit(X_train, y_train)\n",
        "\n",
        "# 7. EXTRACT & EVALUATE\n",
        "best_dt = dt_grid.best_estimator_\n",
        "y_pred_best = best_dt.predict(X_test)\n",
        "\n",
        "print(\"\\nBest Parameters:\", dt_grid.best_params_)\n",
        "print(\"\\nClassification Report:\\n\", classification_report(y_test, y_pred_best, digits=3))\n",
        "\n",
        "\n",
        "# # 1. Get the best model from the grid\n",
        "# best_dt = dt_grid.best_estimator_\n",
        "\n",
        "# # 2. Get predictions and the report\n",
        "# y_pred_best = best_dt.predict(X_test)\n",
        "report_best = classification_report(y_test, y_pred_best, output_dict=True)\n",
        "\n",
        "# 3. Append to your master list\n",
        "# Note: using 'False' as the key because your data labels are 'True'/'False'\n",
        "results.append({\n",
        "    \"model\": \"Decision Tree (Further Tuning)\",\n",
        "    \"cv_f1\": dt_grid.best_score_,\n",
        "    \"test_accuracy\": report_best[\"accuracy\"],\n",
        "    \"test_recall_breach\": report_best[\"False\"][\"recall\"],\n",
        "    \"test_f1_breach\": report_best[\"False\"][\"f1-score\"],\n",
        "    \"best_params\": str(dt_grid.best_params_) # Store as string for the table\n",
        "})\n",
        "\n",
        "# 4. Display the updated table\n",
        "results_df = pd.DataFrame(results)\n",
        "display(results_df)\n",
        "\n"
      ],
