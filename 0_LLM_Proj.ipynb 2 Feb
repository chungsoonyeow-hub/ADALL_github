    {
      "cell_type": "code",
      "source": [
        "## Grid Search for DT\n",
        "\n",
        "import pandas as pd\n",
        "import numpy as np\n",
        "\n",
        "from sklearn.model_selection import StratifiedKFold, GridSearchCV\n",
        "from sklearn.metrics import confusion_matrix, classification_report\n",
        "\n",
        "# #cv = StratifiedKFold(\n",
        "#     n_splits=5,\n",
        "#     shuffle=True,\n",
        "#     random_state=42\n",
        "# )\n",
        "\n",
        "cv = StratifiedKFold(\n",
        "    n_splits=10,\n",
        "    shuffle=True,\n",
        "    random_state=42\n",
        ")\n",
        "param_grid = {\n",
        "    'model__max_depth': [3, 5, 8, None],\n",
        "    'model__min_samples_leaf': [20, 50, 100],\n",
        "    'model__criterion': ['gini', 'entropy']\n",
        "}\n",
        "\n",
        "\n",
        "\n",
        "print(\"=== DECISION TREE: GRID SEARCH CV ===\")\n",
        "\n",
        "dt_grid = GridSearchCV(\n",
        "    estimator=dt_pipeline,   # your EXISTING pipeline\n",
        "    param_grid=param_grid,\n",
        "    cv=cv,\n",
        "    scoring='f1',             # important for imbalanced data\n",
        "    n_jobs=-1,\n",
        "    refit=True,\n",
        "    return_train_score=True\n",
        ")\n",
        "\n",
        "dt_grid.fit(X_train, y_train)\n",
        "\n",
        "## Extract Best Model\n",
        "\n",
        "best_dt = dt_grid.best_estimator_\n",
        "\n",
        "print(\"Best parameters:\")\n",
        "print(dt_grid.best_params_)\n",
        "\n",
        "print(\"\\nBest CV F1 score:\")\n",
        "print(round(dt_grid.best_score_, 3))\n",
        "\n",
        "\n",
        "###########################################################\n",
        "# --- STEP 3: CV diagnostics on TRAIN ONLY ---\n",
        "cv_out = cross_validate(\n",
        "    best_dt,           # The best model from your Grid Search\n",
        "    X_train, y_train,\n",
        "    cv=cv,             # Your StratifiedKFold(n_splits=10)\n",
        "    scoring=\"f1\",\n",
        "    return_train_score=True,\n",
        "    n_jobs=-1\n",
        ")\n",
        "\n",
        "# Calculate the Means\n",
        "cv_f1_val_mean = cv_out[\"test_score\"].mean()\n",
        "cv_f1_train_mean = cv_out[\"train_score\"].mean()\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "######################################################################\n",
        "\n",
        "## Holdout Evaluation\n",
        "\n",
        "y_pred_best = best_dt.predict(X_test)\n",
        "\n",
        "print(\"\\n=== CONFUSION MATRIX (BEST DT) ===\")\n",
        "print(confusion_matrix(y_test, y_pred_best))\n",
        "\n",
        "print(\"\\n=== CLASSIFICATION REPORT (BEST DT) ===\")\n",
        "print(classification_report(y_test, y_pred_best, digits=3))\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "report_best = classification_report(\n",
        "    y_test,\n",
        "    y_pred_best,\n",
        "    output_dict=True\n",
        ")\n",
        "\n",
        "# dt_tuned_record = {\n",
        "#     'model': 'Decision Tree (GridSearch)',\n",
        "#     'cv_f1_best': dt_grid.best_score_,\n",
        "#     'test_accuracy': report_best['accuracy'],\n",
        "#     'test_recall_breach': report_best['False']['recall'],\n",
        "#     'test_f1_breach': report_best['False']['f1-score'],\n",
        "#     'best_params': dt_grid.best_params_\n",
        "# }\n",
        "\n",
        "##########\n",
        "# 1. Get the index of the best parameters\n",
        "idx = dt_grid.best_index_\n",
        "\n",
        "# 2. Extract the \"Lab Book\" Metrics\n",
        "cv_f1_train_mean = dt_grid.cv_results_['mean_train_score'][idx]\n",
        "cv_f1_val_std = dt_grid.cv_results_['std_test_score'][idx]\n",
        "cv_f1_train_std = dt_grid.cv_results_['std_train_score'][idx]\n",
        "\n",
        "# 3. Create the Complete Record\n",
        "dt_tuned_record = {\n",
        "    'model': 'DT (GridSearch) F1',\n",
        "    'cv_f1_val_mean': dt_grid.best_score_,      # Already have this\n",
        "    'cv_std_val': cv_f1_val_std,               # ADDED\n",
        "    'cv_f1_train_mean': cv_f1_train_mean,      # ADDED\n",
        "    'cv_std_train': cv_f1_train_std,           # ADDED\n",
        "    'holdout_f1_breach': report_best['False']['f1-score'],\n",
        "    'holdout_recall_breach': report_best['False']['recall'],\n",
        "    'best_params': dt_grid.best_params_\n",
        "}\n",
        "\n",
        "################\n",
        "\n",
        "dt_tuned_df = pd.DataFrame([dt_tuned_record])\n",
        "dt_tuned_df\n",
        "\n",
        "### append results\n",
        "from sklearn.metrics import classification_report\n",
        "\n",
        "report = classification_report(\n",
        "    y_test,\n",
        "    y_pred,\n",
        "    output_dict=True\n",
        ")\n",
        "\n",
        "\n",
        "results.append({\n",
        "       'model': 'DT(GridSearch) F1',\n",
        "    'cv_f1_val_mean': dt_grid.best_score_,      # Already have this\n",
        "    'cv_std_val': cv_f1_val_std,               # ADDED\n",
        "    'cv_f1_train_mean': cv_f1_train_mean,      # ADDED\n",
        "    'cv_std_train': cv_f1_train_std,           # ADDED\n",
        "    'holdout_f1_breach': report_best['False']['f1-score'],\n",
        "    'holdout_recall_breach': report_best['False']['recall'],\n",
        "    'best_params': dt_grid.best_params_\n",
        "})\n",
        "\n",
        "results_df = pd.DataFrame(results)\n",
        "display(results_df)"
