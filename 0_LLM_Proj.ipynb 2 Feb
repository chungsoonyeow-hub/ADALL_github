              "summary": "{\n  \"name\": \"results_df\",\n  \"rows\": 4,\n  \"fields\": [\n    {\n      \"column\": \"model\",\n      \"properties\": {\n        \"dtype\": \"string\",\n        \"num_unique_values\": 4,\n        \"samples\": [\n          \"Decision Tree (GridSearch)\",\n          \"XGBoost (GridSearch)\",\n          \"Decision Tree (Baseline)\"\n        ],\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    },\n    {\n      \"column\": \"cv_f1\",\n      \"properties\": {\n        \"dtype\": \"number\",\n        \"std\": 0.11885400905386496,\n        \"min\": 0.6239922197799814,\n        \"max\": 0.9136964544818668,\n        \"num_unique_values\": 4,\n        \"samples\": [\n          0.7482438410498966,\n          0.9136964544818668,\n          0.6239922197799814\n        ],\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    },\n    {\n      \"column\": \"test_accuracy\",\n      \"properties\": {\n        \"dtype\": \"number\",\n        \"std\": 0.13965042537739522,\n        \"min\": 0.5030096308186196,\n        \"max\": 0.8441011235955056,\n        \"num_unique_values\": 4,\n        \"samples\": [\n          0.6512841091492777,\n          0.8441011235955056,\n          0.5030096308186196\n        ],\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    },\n    {\n      \"column\": \"test_recall_breach\",\n      \"properties\": {\n        \"dtype\": \"number\",\n        \"std\": 0.32788450034933575,\n        \"min\": 0.05541871921182266,\n        \"max\": 0.7980295566502463,\n        \"num_unique_values\": 4,\n        \"samples\": [\n          0.6490147783251231,\n          0.05541871921182266,\n          0.7980295566502463\n        ],\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    },\n    {\n      \"column\": \"test_f1_breach\",\n      \"properties\": {\n        \"dtype\": \"number\",\n        \"std\": 0.13295524277166793,\n        \"min\"